{
  "name": "web-chat",
  "version": "0.1.0",
  "private": true,
  "main": "./electron.js",
  "homepage": "./",
  "description": "Hello World!",
  "author": "Jane Doe",
  "license": "MIT",
  "dependencies": {
    "@douyinfe/semi-icons": "^2.68.4",
    "@douyinfe/semi-ui": "^2.68.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "websocket": "^1.0.35",
    "ws": "^8.18.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "crypto-js": "^4.2.0",
    "electron": "^33.2.0",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-reloader": "^1.2.3",
    "file-saver": "^2.0.5",
    "webpack-cli": "^5.1.4"
  },
  "build": {
    "appId": "Web-Chat",
    "productName": "Web-Chat",
    "copyright": "MIT",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "nsis": {
      "oneClick": false,
      "language": "2052",
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "icon": "./build/web-chat.png",
      "target": "nsis"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Utility",
      "icon": "./build/web-chat.png",
      "desktop": {
        "Name": "Web-Chat",
        "Comment": "A simple web chat application",
        "Exec": "web-chat",
        "Icon": "web-chat",
        "Type": "Application",
        "StartupNotify": true
      }
    },
    "files": [
      "build/**/*",
      "./electron.js"
    ],
    "extends": null
  }

}

